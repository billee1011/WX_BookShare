@import "../../assets/styles/weui.wxss";
.weui-cells{
    padding: 15px 15px;
}
.weui-cells__bd{
    margin-left: 20rpx
}
.weui-btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    color: #fff;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}

.weui-btn_primary {
    background-color: #1aad19;
}

