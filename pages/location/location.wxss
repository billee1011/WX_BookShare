/* pages/location/location.wxss */
@import "../../assets/styles/weui.wxss";

.weui-cells {
    margin-bottom: 152rpx;
}
weui-cell{
    padding: 20px 15px!important
}

.weui-btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    color: #fff;
    line-height: 2.55555556;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    overflow: hidden;
}

.weui-btn_primary {
    background-color: #1aad19;
}

.bottom {
    position: fixed;
    bottom: 0;
    width: 96%;
    padding: 0 2%;
     background: #fff; 
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}
.weui-check__label{
    border-top:0px solid #fff;
    border-bottom: 1rpx solid #D9D9D9
}